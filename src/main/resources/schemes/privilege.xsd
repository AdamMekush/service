<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://microservice.com/service"
           targetNamespace="http://microservice.com/service" elementFormDefault="qualified">


    <xs:element name="getPrivilegeRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getPrivilegeResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>